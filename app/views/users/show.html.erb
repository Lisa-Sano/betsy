  <div class="panel-heading"><h2>My Info</h2>
    <%= button_to "Edit My Account Info", edit_user_path(current_user), class: "btn btn-default", id: "edit-user", method: :get%>
</div>

  <!-- Table -->
  <table class="table">
    <tr>
    <td><strong>User Name:</strong></td><td><%= current_user.user_name %></td>
